<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/starbackground2"
    tools:context=".MainPage">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="739dp"
        android:layout_height="193dp"
        android:layout_marginEnd="3dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/starbg_nobg" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/learn"
        android:layout_width="0dp"
        android:layout_height="74dp"
        android:layout_marginStart="39dp"
        android:layout_marginEnd="124dp"
        android:layout_marginBottom="50dp"
        android:background="@drawable/custom_button"
        android:padding="2dp"
        android:text="Learn"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/test"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/test"
        android:layout_width="0dp"
        android:layout_height="74dp"
        android:layout_marginEnd="137dp"
        android:layout_marginBottom="50dp"
        android:background="@drawable/custom_button"
        android:padding="2dp"
        android:text="Test"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/repo"
        app:layout_constraintStart_toEndOf="@+id/learn" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/repo"
        android:layout_width="0dp"
        android:layout_height="74dp"
        android:layout_marginEnd="26dp"
        android:layout_marginBottom="50dp"
        android:background="@drawable/custom_button"
        android:text="Repo"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/test" />
</androidx.constraintlayout.widget.ConstraintLayout>